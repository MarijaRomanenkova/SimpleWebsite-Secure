# Terraform Variables Template
# Copy this file to terraform/terraform.tfvars and update with your values

# VPC Configuration
vpc_cidr = "10.0.0.0/16"

# Environment
environment = "production"

# AWS Account ID (will be automatically filled by deploy script)
aws_account_id = "YOUR_AWS_ACCOUNT_ID"

# AWS Region (change to your preferred region)
aws_region = "eu-north-1"
